{
    "name": "サンクスラボ 営業支援ツール",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "ext/index.html"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
        }
    ],
    "permissions": ["storage","activeTab","declarativeContent"]
  }